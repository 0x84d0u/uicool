{
  "name": "@0x84d0u/uicool",
  "version": "0.1.0",
  "description": "A modern React UI library built with Tailwind CSS",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./hooks": {
      "types": "./dist/_EXPORT/hooks.d.ts",
      "import": "./dist/_EXPORT/hooks.mjs",
      "require": "./dist/_EXPORT/hooks.js"
    },
    "./site": {
      "types": "./dist/_EXPORT/site.d.ts",
      "import": "./dist/_EXPORT/site.mjs",
      "require": "./dist/_EXPORT/site.js"
    },
    "./theme": {
      "types": "./dist/_EXPORT/theme.d.ts",
      "import": "./dist/_EXPORT/theme.mjs",
      "require": "./dist/_EXPORT/theme.js"
    },
    "./utils": {
      "types": "./dist/_EXPORT/utils.d.ts",
      "import": "./dist/_EXPORT/utils.mjs",
      "require": "./dist/_EXPORT/utils.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -rf dist && clear && tsup",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "tailwind",
    "tailwindcss",
    "nextjs"
  ],
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19",
    "tailwindcss": "^4"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "react": "^19",
    "react-dom": "^19",
    "tsup": "^8.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@radix-ui/themes": "^3.2.1",
    "@tailwindcss/postcss": "^4.1.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "lucide-react": "^0.546.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "radix-ui": "^1.4.3",
    "tailwind-merge": "^2.2.0"
  }
}